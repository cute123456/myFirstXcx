<view class="containeer">
  <view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="{{index}}" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{items}}" wx:key="" bindtap='toSwiperTap'>
    <view class="content">{{item.content}}</view>
    <view class="del" catchtap="del" data-index="{{index}}">删除</view>
  </view>
</view>
